---
layout: null
permalink: /search-personal.json
---
{%- assign posts = site.posts | where_exp: "post", "post.categories contains 'personal'" -%}
[
{%- for post in posts -%}
  {
    "title": {{ post.title | jsonify }},
    "url": {{ post.url | relative_url | jsonify }},
    "date": "{{ post.date | default: "1900-01-01" | date: "%Y-%m-%d" }}",
    "content": {{ post.content
      | strip_html
      | replace: "\r\n", " "
      | replace: "\n", " "
      | replace: "\r", " "
      | strip
      | jsonify }},
    "categories": {{ post.categories | jsonify }}
  }{%- unless forloop.last -%},{%- endunless -%}
{%- endfor -%}
]
